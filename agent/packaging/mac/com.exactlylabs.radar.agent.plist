<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>Label</key>
        <string>com.exactlylabs.radar.agent</string>
        <key>ServiceDescription</key>
        <string>Service that runs speed tests continually, communicating with radar.exactlylabs.com</string>
        <key>Program</key>
        <string>/Users/mendes/radar-agent</string>
        <!-- <key>ProcessType</key>
        <string>Interactive</string> -->
        <key>LimitLoadToSessionType</key>
        <string>Background</string>
        <key>UserName</key>
        <string>mendes</string>
        <key>StandardOutPath</key>
        <string>/var/log/radar-agent.log</string>
        <key>StandardErrorPath</key>
        <string>/var/log/radar-agent.log</string>
        <key>Debug</key>
        <true/>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <dict>
                <key>SuccessfulExit</key>
                <false/>
                <key>NetworkState></key>
                <false/>
        </dict>
        <key>Disabled</key>
        <false/>
</dict>
</plist>
