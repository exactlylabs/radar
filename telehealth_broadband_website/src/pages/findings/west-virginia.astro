	<CarrouselModal id={'findings-modal'} isOpen={false} elementCount={3}>
		<div class={carrouselModalStyles.carrouselContentCard} data-index="0">
			<StateFindingsModal head="01" title="Broadband Performance Variability" heroImage={findingThumbnail1.src}>
				<div class={styles.twoElementGrid}>
					<BulletPoint
							title="Mixed Connectivity Results"
							content="Median download speeds varied widely across the counties, with some areas, such as Kanawha County, achieving 304.3 Mbps, while others, like Roane County, lagged behind at 15.3 Mbps."
							direction="column"
