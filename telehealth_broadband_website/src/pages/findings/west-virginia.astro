---
import '../../global.css';
import styles from './styles/template.module.css';
import carrouselModalStyles from '../../components/common/wrappers/styles/carrousel_modal.module.css';
import BaseLayout from "../../layouts/BaseLayout.astro";
import StateFindingsHero from "../../components/findings/states/StateFindingsHero.astro";
import StateFindingsOverview from "../../components/findings/states/StateFindingsOverview.astro";
import StateGuidanceAndRecommendations from "../../components/findings/states/StateGuidanceAndRecommendations.astro";
import SectionWithIncomingImage from "../../components/common/wrappers/SectionWithIncomingImage";
import BulletPoint from "../../components/common/content/BulletPoint.astro";
import StateFindings from "../../components/findings/states/StateFindings.astro";
import StateFindingsInANutshell from "../../components/findings/FindingsBottomSection.astro";
import Tab from "../../components/common/misc/Tab.astro";
import FloatingTabs from "../../components/common/misc/FloatingTabs.astro";
import StateFindingsButton from "../../components/findings/states/StateFindingsButton.astro";
import StateFindingsModal from "../../components/findings/states/StateFindingsModal.astro";
import WestVirginiaGuidanceAndRecommendationsContent
  from "../../components/findings/states/west-virginia/WestVirginiaGuidanceAndRecommendationsContent.astro";

import findingsHero from '../../assets/images/wv-findings-hero.webp';
import stateImage from '../../assets/images/wv-state-image.webp';
import findingThumbnail1 from '../../assets/images/wv-findings-thumbnail-1.webp';
import findingThumbnail2 from '../../assets/images/wv-findings-thumbnail-2.webp';
import findingThumbnail3 from '../../assets/images/wv-findings-thumbnail-3.webp';
import outreachStrategies from '../../assets/images/wv-vertical-incoming-1.webp';
import lessonsLearned from '../../assets/images/wv-vertical-incoming-2.webp';
import mainStateImage from '../../assets/images/wv-main-findings-image.webp';
import CarrouselModal from "../../components/common/wrappers/CarrouselModal.astro";
---

<BaseLayout
        title="Findings in West Virginia"
        description="Findings in West Virginia"
        heroBgStyle={{ backgroundImage: `url(${findingsHero.src})`, height: '536px' }}
>
  <StateFindingsHero
          state="West Virginia"
          head="Findings in West Virginia"
          title="West Virginia's Connectivity Landscape"
          subtitle="Highlighting the effects of broadband limitations on healthcare connectivity in West Virginia."
          reportUrl="/findings/west-virginia-report.pdf"
  />

  <StateFindingsOverview
          state="West Virginia"
          title="The Study in West Virginia"
          firstParagraph="In West Virginia, the Telehealth Broadband Pilot focused on seven counties: Calhoun, Clay, Jackson, Kanawha, Nicholas, Ritchie, and Roane."
          secondParagraph="West Virginia's mountainous terrain and dispersed rural communities posed significant barriers to broadband deployment. Despite some progress, inconsistent and unreliable connectivity remains a significant obstacle to telehealth adoption across the region for both healthcare organizations and consumers."
          stateImageUrl={stateImage.src}
  />

  <div class={styles.mainBodyImageContainer} id="horizontal-shift-image">
    <img src={mainStateImage.src} alt="West Virginia landscape" loading="eager"></img>
  </div>

  <StateFindings
          title="Insights in Rural West Virginia"
          subtitle="Explore the broadband gaps, telehealth challenges, and opportunities for community-driven solutions."
  >
    <StateFindingsButton
            modalId="findings-modal"
            backgroundImage={findingThumbnail1.src}
            head="01"
            title="Broadband Performance Variability"
            index="0"
    />
    <StateFindingsButton
            modalId="findings-modal"
            backgroundImage={findingThumbnail2.src}
            head="02"
            title="Healthcare Connectivity Challenges"
            index="1"
    />
    <StateFindingsButton
            modalId="findings-modal"
            backgroundImage={findingThumbnail3.src}
            head="03"
            title="Community Anchor Institutions as Potential Hubs"
            index="2"
    />
  </StateFindings>

  <StateGuidanceAndRecommendations title="Strategies and Learnings from West Virginia">
    <FloatingTabs scrollToId="guidance-and-recommendations">
      <Tab
              id="outreach-strategies"
              label="Outreach Strategies"
              isActive="true"
      >
