---
import '../../global.css';
import styles from './styles/implementation_learnings.module.css';
import commonStyles from '../../common.module.css';
import BaseLayout from "../../layouts/BaseLayout.astro";
import HeroWithText from "../../components/common/content/HeroWithText.astro";
import FloatingTabs from "../../components/common/misc/FloatingTabs.astro";
import Tab from "../../components/common/misc/Tab.astro";
import SectionWithIncomingImage from "../../components/common/wrappers/SectionWithIncomingImage";
import TransparentSecondaryButton from "../../components/common/buttons/TransparentSecondaryButton.astro";
import ImplementationLearningsIncomingImageContent
  from "../../components/findings/implementation-learnings/ImplementationLearningsIncomingImageContent.astro";

import heroBgImage from '../../assets/images/implementation-learnings-hero.jpg';
import evaluationResultsLights from '../../assets/images/evaluation-results-lights.webp';
import implementationLearningsResults from '../../assets/images/implementation-learnings-results.webp';
import guidanceLight from '../../assets/images/state-findings-guidance-light.webp';
import engagementStrategies from '../../assets/images/implementation-learnings-engagement-strategies.jpg';
import deploymentMethods from '../../assets/images/implementation-learnings-deployment-methods.jpg';
import motivatingParticipation from '../../assets/images/implementation-learnings-motivating-participation.jpg';
import StateFindingsInANutshell from "../../components/findings/FindingsBottomSection.astro";
import BulletPoint from "../../components/common/content/BulletPoint.astro";
---

<BaseLayout
        title="Implementation Learnings"
        description="Implementation Learnings"
        heroBgStyle={{backgroundImage: `url(${heroBgImage.src})`, height: '536px'}}
>
    <section class={commonStyles.paddedSection}>
        <div class={styles.heroContainer}>
            <HeroWithText head="Implementation Learnings"
                          title="Strategies for Rural Data Collection"
                          content="Lessons learned from innovative approaches to gathering broadband data in rural communities."
            />
        </div>
        <div class={styles.downloadButtonContainer}>
            <TransparentSecondaryButton href={'/downloads/implementation-findings.pdf'} download>
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="16" height="16">
                    <path fill="white" transform="translate(0.25 0.25)" d="M7.75 0C8.1642132 0 8.5 0.33578643 8.5 0.75L8.5 9.9720001L11.21967 7.2530031C11.485936 6.9867368 11.9026 6.9625306 12.196212 7.1803851L12.28033 7.2530031C12.573223 7.5458965 12.573223 8.0207701 12.28033 8.3136635L8.2803297 12.313663C7.9874368 12.606557 7.5125632 12.606557 7.2196698 12.313663L3.2196698 8.3136635C2.9267766 8.0207701 2.9267766 7.5458965 3.2196698 7.2530031C3.5125632 6.9601102 3.9874368 6.9601102 4.2803302 7.2530031L7 9.9720001L7 0.75C7 0.33578643 7.3357863 0 7.75 0ZM14.75 14C15.164213 14 15.5 14.335787 15.5 14.75C15.5 15.164213 15.164213 15.5 14.75 15.5L0.75 15.5C0.33578643 15.5 0 15.164213 0 14.75C0 14.335787 0.33578643 14 0.75 14L14.75 14Z"/>
                </svg>
                Download Full Report
            </TransparentSecondaryButton>
        </div>
    </section>


    <section class={`${commonStyles.paddedSection} ${styles.evaluationResultsSection}`}>
        <div class={styles.evaluationResultsTop}>
            <div class={styles.evaluationResultsHeroContainer}>
                <HeroWithText head="Overview"
                              title="Guiding Future Rural Broadband Projects"
                              content="In collaboration with ANTHC and local Community Lead Partners (CLPs), our implementation efforts leveraged diverse data collection methods designed to address rural challenges. From in-person outreach to advanced remote and mobile tools, these strategies tackled regional barriers and uncovered insights to guide future rural broadband projects."
                />
            </div>
            <div class={styles.sidePicturesContainer}>
                <img src={implementationLearningsResults.src} alt="Evaluation Results hero image"/>
            </div>
        </div>
    </section>

    <section class={`${commonStyles.paddedSection} ${styles.scrollerSection}`}>
        <img src={guidanceLight.src} class={styles.light}/>
        <div class={styles.guidanceHeroContainer}>
            <HeroWithText head={'Guidance & Recommendations'} title={'Strategies and Learnings from West Virginia'}/>
        </div>
        <img src={evaluationResultsLights.src} alt="Evaluation results lights" class={styles.scrollerLights} />
        <FloatingTabs scrollToId="guidance-and-recommendations">
            <Tab
                    id="engagement-strategies"
                    label="Engagement Strategies"
                    isActive="true"
            >
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24" height="24">
