---
import '../../global.css';
import styles from './styles/template.module.css';
import BaseLayout from "../../layouts/BaseLayout.astro";
import StateFindingsHero from "../../components/findings/states/StateFindingsHero.astro";
import StateFindingsOverview from "../../components/findings/states/StateFindingsOverview.astro";
import StateGuidanceAndRecommendations from "../../components/findings/states/StateGuidanceAndRecommendations.astro";
import SectionWithIncomingImage from "../../components/common/wrappers/SectionWithIncomingImage";
import BulletPoint from "../../components/common/content/BulletPoint.astro";
import StateFindings from "../../components/findings/states/StateFindings.astro";
import StateFindingsInANutshell from "../../components/findings/FindingsBottomSection.astro";
import Tab from "../../components/common/misc/Tab.astro";
import FloatingTabs from "../../components/common/misc/FloatingTabs.astro";
import AlaskaGuidanceAndRecommendationsContent
  from "../../components/findings/states/alaska/AlaskaGuidanceAndRecommendationsContent.astro";
import Modal from "../../components/common/wrappers/Modal.astro";
import StateFindingsButton from "../../components/findings/states/StateFindingsButton.astro";
import StateFindingsModal from "../../components/findings/states/StateFindingsModal.astro";

import findingsHero from '../../assets/images/alaska-findings-hero.webp';
import stateImage from '../../assets/images/alaska-state-image.webp';
import findingThumbnail1 from '../../assets/images/alaska-findings-thumbnail-1.webp';
import findingThumbnail2 from '../../assets/images/alaska-findings-thumbnail-2.webp';
import findingThumbnail3 from '../../assets/images/alaska-findings-thumbnail-3.webp';
import outreachStrategies from '../../assets/images/alaska-vertical-incoming-1.webp';
import lessonsLearned from '../../assets/images/alaska-vertical-incoming-2.webp';
import mainStateImage from '../../assets/images/alaska-main-findings-image.webp';
---

<BaseLayout
  title="Findings in Alaska"
  description="Findings in Alaska"
  heroBgStyle={{ backgroundImage: `url(${findingsHero.src})`, height: '536px' }}
>
  <StateFindingsHero
    state="Alaska"
    head="Findings in Alaska"
    title="Broadband Challenges in Remote Alaska"
    subtitle="Exploring the impact of extreme geography and weather on telehealth connectivity in Alaska's most remote regions."
    reportUrl="/findings/alaska-report.pdf"
  />
  
  <StateFindingsOverview
    state="Alaska"
    title="The Program in Alaska"
    firstParagraph="The Telehealth Broadband Pilot Program in Alaska focused on six remote boroughs and census areas: Aleutians West, Bristol Bay, Dillingham, Nome, North Slope, and Northwest Arctic."
    secondParagraph="Alaska's unique geography and extreme weather conditions posed significant challenges to broadband infrastructure. Despite efforts, most locations fell below the standards required for reliable telehealth services, emphasizing the need for tailored solutions to overcome these barriers."
    stateImageUrl={stateImage.src}
  />
  
  <div class={styles.mainBodyImageContainer} id="horizontal-shift-image">
    <img src={mainStateImage.src} alt="Alaska landscape" loading="eager" />
  </div>
  
  <StateFindings
    title="Insights in Rural Alaska"
    subtitle="Explore broadband access and gaps, telehealth challenges, and opportunities for community-driven solutions."
  >
    <StateFindingsButton
      modalId="broadband-performance-variability"
      backgroundImage={findingThumbnail1.src}
      head="01"
      title="Broadband Performance Variability"
    />
    <StateFindingsButton
      modalId="healthcare-connectivity-challenges"
      backgroundImage={findingThumbnail2.src}
      head="02"
      title="Healthcare Connectivity Challenges"
    />
    <StateFindingsButton
      modalId="community-anchor-institutions-as-potential-hubs"
      backgroundImage={findingThumbnail3.src}
      head="03"
      title="Community Anchor Institutions as Potential Hubs"
    />
  </StateFindings>
  
  <StateGuidanceAndRecommendations
    title="Implementation Insights for Alaska"
  >
    <FloatingTabs scrollToId="guidance-and-recommendations">
      <Tab
        id="outreach-strategies"
        label="Outreach Strategies"
        isActive="true"
      >
