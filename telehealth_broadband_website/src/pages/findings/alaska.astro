  <Modal id={'broadband-performance-variability'} isOpen={false} clearPaddings bgColor="#0F0E25">
    <StateFindingsModal head="01" title="Broadband Performance Variability" heroImage={findingThumbnail1.src}>
      <div class={styles.twoElementGrid}>
        <BulletPoint
          title="Very Low Overall Speeds"
          content="Median broadband speeds across healthcare sites, consumers, and other Community Anchor Institutions (CAls) were among the lowest across all TBP states. Healthcare locations reported average median download speeds of 27.1 Mbps, well below the 100 Mbps standard."
          direction="column"
