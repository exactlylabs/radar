---
import '../../global.css';
import styles from './styles/template.module.css';
import BaseLayout from "../../layouts/BaseLayout.astro";
import StateFindingsHero from "../../components/findings/states/StateFindingsHero.astro";
import StateFindingsOverview from "../../components/findings/states/StateFindingsOverview.astro";
import StateGuidanceAndRecommendations from "../../components/findings/states/StateGuidanceAndRecommendations.astro";
import SectionWithIncomingImage from "../../components/common/wrappers/SectionWithIncomingImage";
import BulletPoint from "../../components/common/content/BulletPoint.astro";
import StateFindings from "../../components/findings/states/StateFindings.astro";
import StateFindingsInANutshell from "../../components/findings/states/StateFindingsInANutshell.astro";
import Tab from "../../components/common/misc/Tab.astro";
import FloatingTabs from "../../components/common/misc/FloatingTabs.astro";
import Modal from "../../components/common/wrappers/Modal.astro";
import StateFindingsButton from "../../components/findings/states/StateFindingsButton.astro";
import StateFindingsModal from "../../components/findings/states/StateFindingsModal.astro";

import findingsHero from '../../assets/images/texas-findings-hero.webp';
import stateImage from '../../assets/images/texas-state-image.webp';
import findingThumbnail1 from '../../assets/images/texas-findings-thumbnail-1.webp';
import findingThumbnail2 from '../../assets/images/texas-findings-thumbnail-2.webp';
import findingThumbnail3 from '../../assets/images/texas-findings-thumbnail-3.webp';
import findingThumbnail4 from '../../assets/images/texas-findings-thumbnail-4.webp';
import outreachStrategies from '../../assets/images/texas-vertical-incoming-1.webp';
import dataCollectionMethods from '../../assets/images/texas-vertical-incoming-2.webp';
import lessonsLearned from '../../assets/images/texas-vertical-incoming-3.webp';
import mainStateImage from '../../assets/images/texas-main-findings-image.webp';
import TexasGuidanceAndRecommendationsContent
  from "../../components/findings/states/texas/TexasGuidanceAndRecommendationsContent.astro";
---

<BaseLayout
        title="Findings in Texas"
        description="Findings in Texas"
        heroBgStyle={{ backgroundImage: `url(${findingsHero.src})`, height: '536px' }}
>
  <StateFindingsHero
          state="Texas"
          head="Findings in Texas"
          title="A Look Into the Rural Texas Communities"
          subtitle="Texas had strong broadband at Community Anchor Institutions but struggled with widespread consumer broadband as well as healthcare sites."
          reportUrl="/findings/texas-report.pdf"
  />

  <StateFindingsOverview
          state="Texas"
          title="The Study in Texas"
          firstParagraph="The Telehealth Broadband Pilot focused on six rural counties in Texas: Crosby, Fisher, Haskell, Jones, Lamb, and Mitchell."
          secondParagraph="While some institutions meet connectivity standards, many residents and healthcare providers face inconsistent and often insufficient internet performance. Addressing these challenges through targeted infrastructure improvements and leveraging the strengths of Community Anchor Institutions can enhance telehealth accessibility, ultimately improving healthcare outcomes in rural Texas communities."
          stateImageUrl={stateImage.src}
          styleOverride="--aspect-ratio: 440 / 418; max-width: 440px;"
  />

  <div class={styles.mainBodyImageContainer} id="horizontal-shift-image">
    <img src={mainStateImage.src} alt="Texas landscape" loading="eager"></img>
  </div>
  
  <StateFindings
          title="Insights in Rural Texas"
          subtitle="Explore the broadband gaps, telehealth challenges, and opportunities for community-driven solutions."
          mosaic
  >
    <StateFindingsButton
            modalId="broadband-performance-variability"
            backgroundImage={findingThumbnail1.src}
            head="01"
            title="Broadband Performance Variability"
    />
    <StateFindingsButton
            modalId="consumer-access-below-telehealth-standards"
            backgroundImage={findingThumbnail2.src}
            head="02"
            title="Consumer Access Below Telehealth Standards"
    />
    <StateFindingsButton
            modalId="healthcare-connectivity-challenges"
            backgroundImage={findingThumbnail3.src}
            head="03"
            title="Healthcare Connectivity Challenges"
    />
    <StateFindingsButton
            modalId="community-anchor-institutions-as-potential-hubs"
            backgroundImage={findingThumbnail4.src}
            head="04"
            title="Community Anchor Institutions as Potential Hubs"
    />
  </StateFindings>

  <StateGuidanceAndRecommendations title="Strategies and Learnings from West Virginia">
